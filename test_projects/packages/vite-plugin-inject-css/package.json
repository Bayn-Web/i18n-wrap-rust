{
    "name": "vite-plugin-inject-css",
    "version": "0.0.1",
    "description": "A Vite plugin that takes the CSS and adds it to the page through the JS. For those who want a single JS file.",
    "publishConfig": {
        "registry": "http://192.168.100.157:8081/repository/npm-hosted"
    },
    "main": "dist/index.js",
    "type": "module",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "types": "./dist/declarations/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "typings": "dist/declarations/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "vitest",
        "coverage": "rimraf coverage && vitest run --coverage",
        "ci-test": "vitest run",
        "build": "rimraf dist && tsc -p tsconfig.json",
        "format": "prettier '{src,test}/*.ts' --write"
    },
    "keywords": [
        "vite",
        "vite-plugin",
        "plugin",
        "typescript",
        "css-injected-by-js",
        "single-js-file",
        "css",
        "js"
    ],
    "peerDependencies": {
        "vite": ">4.0.0-0"
    },
    "devDependencies": {
        "@types/node": "20.14.11",
        "@vitest/coverage-istanbul": "2.0.3",
        "happy-dom": "14.12.3",
        "prettier": "3.3.3",
        "rimraf": "6.0.1",
        "rollup": "4.18.1",
        "typescript": "5.5.3",
        "vite": "5.3.4",
        "vitest": "2.0.3"
    },
    "license": "MIT"
}