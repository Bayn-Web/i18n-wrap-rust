  "description": "数据备份",
  "author": {
    "name": "wjl",
    "email": "guodong114@hotmail.com",
    "url": "https://github.com/SKT1yang"
  },
  "publishConfig": {
    "registry": "http://192.168.100.157:8081/repository/npm-hosted"
  },
  "license": "MIT",
  "main": "./es/index.js",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "require": "./cjs/index.js",
      "import": "./es/index.js"
    }
  },
  "types": "./es/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "es",
    "cjs",
    "src"
  ],
  "scripts": {
    "build": "pnpm cross-env NODE_ENV=production vite build",
    "clean": "pnpm rimraf dist pnpm-lock.yaml node_modules",
    "dev": "pnpm --filter test dev",
    "serve": "pnpm dev",
    "lint": "pnpm lint:eslint & pnpm lint:stylelint & pnpm lint:prettier",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,test}/**/*.{vue,ts,tsx,js,jsx}\" --fix",
    "lint:prettier": "prettier --write  \"{src,test}/**/*.{js,json,ts,jsx,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix ",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preinstall": "npx only-allow pnpm",
    "postpublish": "glsec check -p ../"
  },
  "dependencies": {
    "@guolisec/request": "0.3.0-beta.1",
    "@guolisec/toast": "0.1.0",
    "@guolisec/types": "0.1.7",
    "@guolisec/utils": "0.2.0-beta.1",
    "@voerkai18n/runtime": "2.1.9",
    "ant-design-vue": "4.2.1",
    "dayjs": "1.11.11",
    "vue": "3.4.27"
  },
  "devDependencies": {
    "@guolisec/eslint-config": "0.1.1",
    "@guolisec/prettier-config": "0.1.1",
    "@guolisec/ts-config": "0.1.0",
    "@guolisec/vite-config": "0.3.0-beta.2",
    "cross-env": "7.0.3",
    "rimraf": "5.0.7",
    "typescript": "5.4.5",
    "vite": "5.2.12",
    "vue-tsc": "2.0.19"
  },
  "voerkai18n": {
    "entry": "entry/languages"
  }
}