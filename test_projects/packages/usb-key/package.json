    "@guolisec/prettier-config": "0.1.1",
    "@guolisec/ts-config": "0.1.0",
    "@guolisec/types": "0.1.7",
    "@guolisec/vite-config": "0.3.0-beta.2",
    "cross-env": "7.0.3",
    "typescript": "5.4.5",
    "vite": "5.2.12",
    "vue-tsc": "2.0.19"
  }
}