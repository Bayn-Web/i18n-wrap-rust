  "main": "./es/index.js",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "require": "./cjs/index.js",
      "import": "es/index.js"
    }
  },
  "types": "./es/index.d.ts",
  "type": "module",
  "files": [
    "dist",
