  "scripts": {
    "build": "pnpm cross-env NODE_ENV=production vite build",
    "clean": "pnpm rimraf dist pnpm-lock.yaml node_modules",
    "dev": "pnpm --filter test dev",
    "serve": "pnpm run dev",
    "lint": "pnpm lint:eslint & pnpm lint:stylelint & pnpm lint:prettier",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,test}/**/*.{vue,ts,tsx,js,jsx}\" --fix",
    "lint:prettier": "prettier --write  \"{src,test}/**/*.{js,json,ts,jsx,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix",
    "report": "pnpm cross-env REPORT=true npm run build",
    "server:br": "pnpx http-server dist --cors --brotli -c-1",
    "server:gzip": "pnpx http-server dist --cors --gzip -c-1",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "postpublish": "glsec check -p ../"
  },
  "dependencies": {
    "@guolisec/request": "0.3.0-beta.1",
    "@guolisec/toast": "0.1.0",
