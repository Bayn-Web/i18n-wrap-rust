  "type": "module",
  "scripts": {
    "build": "pnpm cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 vite build",
    "clean": "pnpm rimraf dist pnpm-lock.yaml node_modules",
    "dev": "vite",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "report": "pnpm cross-env REPORT=true npm run build",
    "serve": "pnpm run dev",
    "server:br": "pnpx http-server dist --cors --brotli -c-1",
    "server:gzip": "pnpx http-server dist --cors --gzip -c-1",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@guolisec/configable": "0.1.5",
    "@guolisec/permission": "0.4.0-beta.3",
    "@guolisec/request": "0.3.0-beta.1",
    "@guolisec/reset.css": "0.1.1",
    "@guolisec/routerable": "0.3.0-beta.1",
    "@guolisec/scheduler": "0.2.2",
    "@guolisec/storable": "0.1.1",
    "@guolisec/theme": "0.2.1",
    "@guolisec/toast": "0.1.0",
    "@guolisec/utils": "0.2.0-beta.1",
    "@voerkai18n/runtime": "2.1.8",
    "ant-design-vue": "4.2.1",
    "pinia": "2.1.7",
    "vue": "3.4.27",
    "vue-router": "4.3.2"
  },
  "devDependencies": {
    "@guolisec/ts-config": "0.1.0",
    "@guolisec/types": "0.1.10",
    "@guolisec/vite-config": "0.3.1-beta.7",
    "cross-env": "7.0.3",
    "typescript": "5.4.5",
    "vite": "5.2.12",
    "vue-tsc": "2.0.21"
  },
  "voerkai18n": {
    "entry": "entry/languages"
  }
}
